open solid


let external_link (props : list prop_) =
    box [
        Children fun () => ;[
            anchor <| props /@ [
                External true
                Children fun () => ;[
                    str <| props /. .Href /? ""
                ]
            ]
            str "&nbsp;"
            icon [
                As <| biRegularLinkExternal_ ()
            ]
        ]
    ]
