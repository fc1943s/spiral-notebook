type account__ = {alias : string; address : string; private_key : string; mnemonic : string}
type state__ =
    {
        chain : {
            token : option string
            url : option string
            port : option i32
            accounts : a i32 account__
        }
        db : {
            status : option any
            connection : option any
        }
        ui : {
            modal : option i32
        }
        profile : {
            tmp : {
                test : option string
            }
        }
    }

inl empty_state () : state__ = join
    {
        chain = {
            token = Some (tictactoe_testnet.token ())
            url = Some (tictactoe_testnet.url ())
            port = Some (tictactoe_testnet.port ())
            accounts = tictactoe_testnet.accounts ()
        }
        db = {
            status = None
            connection = None
        }
        ui = {
            modal = None
        }
        profile = {
            tmp = {
                test = None
            }
        }
    }
